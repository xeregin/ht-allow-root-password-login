for i in `cat devices | sed '/bad/d' | awk '{print $2}'`; do
    sshpass -p 'insecure' scp -o StrictHostKeyChecking=no sshd-allow-root-password-login ubuntu@$i:~
    sshpass -p 'insecure' ssh -o StrictHostKeyChecking=no ubuntu@$i 'chmod u+x sshd-allow-root-password-login'
    sshpass -p 'insecure' ssh -o StrictHostKeyChecking=no ubuntu@$i '~/sshd-allow-root-password-login'
done
